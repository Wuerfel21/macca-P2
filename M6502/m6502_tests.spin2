PUB test_bcc() | addr

    ser.str(string("test_bcc ... "))

    REG_P := %00000000

    REG_PC := $0200
    exec_branch($90, $10, $FF) ' BCC
    if \assertWordEquals($0202 + $10, REG_PC)
        REG_PC := $0200
        registers()
        disassemble()
        abort(0)

    REG_PC := $0200
    exec_branch($B0, $10, $FF) ' BCS
    if \assertWordEquals($0202, REG_PC)
        REG_PC := $0200
        registers()
        disassemble()

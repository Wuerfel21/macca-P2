{
    MOS6502 Emulator Tests
    Written by Marco Maccaferri <macca@maccasoft.com>
}

CON
    _CLKFREQ = 160_000_000

VAR

    ' Processor Instance

    long ctl    '  [19:0] = memory base
                '    [24] = clock enable
                '    [25] = int
                '    [26] = nmi
                '    [27] = single step

    long rdfunc '  [19:0] = memory read function
    long wrfunc '  [19:0] = memory write function
    long rwaddr '  [15:0] = r/w address

    byte REG_A  '   [7:0] = A
    byte REG_X  '  [15:8] = X
    byte REG_Y  ' [23:16] = Y
    byte REG_P  ' [31:24] = P

    word REG_PC '  [15:0] = PC
    word REG_S  ' [23:16] = S

    long stat1  '  [31:0] = total cycles count
    long CPU_T  '   [7:0] = last istruction cycles

    byte ram_6502[65536]

OBJ

    cpu : "m6502"
    ser : "com.serial"

PUB main()

    ser.start(115_200)
    ser.rx()

    ram_6502[$FFFC] := $00
    ram_6502[$FFFD] := $02

    ctl := @ram_6502
    ctl.[cpu.CTL_CLK] := ctl.[cpu.CTL_STEP] := 1
    cpu.start(@ctl)
    repeat while ctl.[cpu.CTL_CLK] == 1

    \run_tests()

    repeat
        waitct(0)

PRI run_tests()

    ser.str(string(13, 10, "run tests", 13, 10))

    test_lda()
    test_ldx()
    test_ldy()

    test_zpg_addressing()
    test_abs_addressing()
    test_abs_x_addressing()
    test_abs_y_addressing()
    test_ind_x_addressing()
    test_ind_y_addressing()
    test_zpg_x_addressing()

    test_adc()
    test_adc_dec()
    test_sbc()
    test_sbc_dec()

    test_inc()
    test_dec()

    test_and()
    test_ora()
    test_eor()
    test_bit()
    test_cmp()

    test_asl()
    test_lsr()
    test_rol()
    test_ror()

    test_pha()
    test_php()

    test_bcc()
    test_bcs()
    test_beq()
    test_bne()
    test_bmi()
    test_bpl()
    test_bvc()
    test_bvs()

    test_clear_flags()
    test_set_flags()

    test_jsr()
    test_brk()

    test_stx_all()
    test_axs()

    test_cycles()

    ser.str(string("done!", 13, 10))

PUB test_lda()

    ser.str(string("test_lda ... "))

    REG_P := %00000000
    REG_A := REG_X := REG_Y := $00
    exec($A9, $1A, $FF) ' LDA #$1A
    if \assertEquals($1A, REG_A) or \assertBinEquals(%00000000, REG_P)
        disassemble()
        registers()
        abort(0)

    exec($A9, $00, $FF) ' LDA #$00
    if \assertEquals($00, REG_A) or \assertBinEquals(%00000010, REG_P)
        disassemble()
        registers()
        abort(0)

    exec($A9, $8A, $FF) ' LDA #$8A
    if \assertEquals($8A, REG_A) or \assertBinEquals(%10000000, REG_P)
        disassemble()
        registers()
        abort(0)

    ser.str(string("Ok", 13, 10))


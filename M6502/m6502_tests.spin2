        registers()
        abort(0)

    REG_A := %11111111
    exec($29, %10000000, $FF) ' AND
    if \assertBinEquals(%10000000, REG_A) or \assertBinEquals(%10000000, REG_P)
        disassemble()
        registers()
        abort(0)

    ser.str(string("Ok", 13, 10))


PRI dump(addr, count)

    repeat count
        ser.fstr1(string("$%2.2x "), peek(addr++))
    if count < 3
        repeat 3 - count
             ser.str(string("    "))

PRI signed(n) : rc

    rc := n < $80 ? n : -(256 - n)
